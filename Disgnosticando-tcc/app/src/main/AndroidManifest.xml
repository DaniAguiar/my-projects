<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tcc.diagnosticando">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".screens.questions.ListQuestions"></activity>
        <activity android:name=".screens.questions.CreateQuestions" />
        <activity android:name=".screens.questions.Questions" />
        <activity android:name=".screens.DiagnosisResult" />
        <activity android:name=".screens.principal.Appointment" />
        <activity android:name=".screens.principal.ChooseProfile" />
        <activity android:name=".screens.principal.Profiles" />
        <activity android:name=".screens.principal.Register" />
        <activity android:name=".screens.principal.Principal">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>